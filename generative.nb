                 RowBox[{"BezierCurve", "[", 
                  RowBox[{"Table", "[", 
                   RowBox[{
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{"RandomReal", "[", 
                    RowBox[{"xAxis", "/", "10"}], "]"}], ",", 
                    RowBox[{"RandomReal", "[", 
                    RowBox[{"yAxis", "/", "10"}], "]"}]}], "}"}], ",", "4"}], 
                   "]"}], "]"}], ",", "randomColor", ",", 
                 RowBox[{"Thickness", "[", 
                  RowBox[{"RandomReal", "[", 
                   RowBox[{"{", 
                    RowBox[{"0.015", ",", "0.025"}], "}"}], "]"}], "]"}]}], 
                "]"}], ",", "curveNum"}], "]"}]}], "]"}], "]"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"RasterSize", "\[Rule]", 
