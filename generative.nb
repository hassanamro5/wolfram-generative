             RowBox[{"{", 
              RowBox[{
               RowBox[{"Darker", "[", 
                RowBox[{
                 RowBox[{"RandomColor", "[", "]"}], ",", "0.5"}], "]"}], ",", 
               RowBox[{"Darker", "[", 
                RowBox[{"Gray", ",", 
                 RowBox[{"2", "/", "3"}]}], "]"}]}], "}"}], ",", "0.75"}], 
            "]"}]}]}], "]"}], ",", "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Rasterize", "[", 
           RowBox[{
            RowBox[{"Style", "[", 
