                    RowBox[{"RandomInteger", "[", 
                    RowBox[{"yAxis", "/", "10"}], "]"}]}], "}"}], ",", 
                    RowBox[{"RandomInteger", "[", "10", "]"}]}], "]"}], "]"}],
