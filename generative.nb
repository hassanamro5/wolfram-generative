    RowBox[{
     RowBox[{"image", ":=", 
      RowBox[{"ImageCompose", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Rasterize", "[", 
         RowBox[{
          RowBox[{"Graphics", "[", 
           RowBox[{"Join", "[", "\[IndentingNewLine]", 
