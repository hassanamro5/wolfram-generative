                    RowBox[{"RandomInteger", "[", 
                    RowBox[{"yAxis", "/", "10"}], "]"}]}], "}"}], ",", 
                   RowBox[{"RandomReal", "[", 
                    RowBox[{"yAxis", "/", "50"}], "]"}]}], "]"}], ",", 
                 "randomColor", ",", 
                 RowBox[{"Thickness", "[", 
                  RowBox[{"RandomReal", "[", 
                   RowBox[{"{", 
                    RowBox[{"0.025", ",", "0.15"}], "}"}], "]"}], "]"}]}], 
                "]"}], ",", "circleNum"}], "]"}], ",", "\[IndentingNewLine]", 
             RowBox[{"Table", "[", 
              RowBox[{
               RowBox[{"Style", "[", 
                RowBox[{
                 RowBox[{"Disk", "[", 
                  RowBox[{
                   RowBox[{"{", 
                    RowBox[{
                    RowBox[{"RandomInteger", "[", 
                    RowBox[{"xAxis", "/", "10"}], "]"}], ",", 
                    RowBox[{"RandomInteger", "[", 
                    RowBox[{"yAxis", "/", "10"}], "]"}]}], "}"}], ",", 
                   RowBox[{"RandomReal", "[", 
                    RowBox[{"yAxis", "/", "50"}], "]"}], ",", 
                   RowBox[{"RandomReal", "[", 
                    RowBox[{
                    RowBox[{"2", " ", "\[Pi]"}], ",", "2"}], "]"}]}], "]"}], 
